{
    "dependencies": {
        "@octokit/rest": "^21.0.0",
        "esno": "^4.7.0",
        "simple-git": "^3.25.0",
        "typescript": "^5.5.2"
    },
    "name": "flakewatch-backend",
    "version": "1.0.0",
    "description": "Live tracking of flaky tests in large public repositories.",
    "main": "flakewatch.js",
    "type": "module",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "esno flakewatch.ts",
        "start": "npm run build && node dist/flakewatch.js",
        "build": "tsc",
        "postinstall": "mkdir -p /home/flakewatch/clone && mkdir -p /home/flakewatch/ci-logs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NateLevin1/flakewatch.git"
    },
    "author": "UltimateProGrammer",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NateLevin1/flakewatch/issues"
    },
    "homepage": "https://github.com/NateLevin1/flakewatch#readme",
    "devDependencies": {
        "@types/node": "^20.14.9"
    }
}
